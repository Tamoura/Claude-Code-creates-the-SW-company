    "@fastify/rate-limit": "^10.3.0",
    "@prisma/client": "^5.8.1",
    "bcrypt": "^6.0.0",
    "fastify": "^5.7.2",
    "fastify-plugin": "^4.5.1",
    "ioredis": "^5.3.2",
    "pino": "^8.17.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
