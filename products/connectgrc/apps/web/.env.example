# ConnectGRC Web App Environment Variables
# Copy this file to .env.local and update values for your environment.
# NEVER commit the .env.local file to version control.

# API URL (must match the backend PORT)
NEXT_PUBLIC_API_URL=http://localhost:5006

# App URL (for metadata, OAuth callbacks)
NEXT_PUBLIC_APP_URL=http://localhost:3110

# Server port
PORT=3110
